<Window x:Class="unsplashAlternative.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:unsplashAlternative"
        xmlns:vm="clr-namespace:unsplashAlternative.ViewModel.helpers"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <vm:UnSplashVM x:Key="vm"/>
    </Window.Resources>
    <Grid DataContext="{StaticResource vm}">
        <Button Command="{Binding SearchCommand}" Margin="0,0,589,306"></Button>
        <Image Margin="290,112,145,49" Source="{Binding BitmapImages[0]}"></Image>
    </Grid>
</Window>
